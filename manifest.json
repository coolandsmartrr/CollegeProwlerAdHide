{
  "name": "CollegeProwler AdHide",
  "description": "Removes ads from CollegeProwler",
  "version": "2.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "24": "images/logo24.png",
    "128": "images/logo128.png"
  },
  "browser_action": {
    "default_title": "Remove ads from CollegeProwler",
    "default_icon": {
      "19": "images/logo19.png",
      "38": "images/logo38.png"
    }
  },
  "manifest_version": 2
}
